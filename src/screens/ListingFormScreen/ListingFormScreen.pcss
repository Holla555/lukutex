          display: flex;
          flex-direction: column;
          justify-content: space-between;
          /* box-shadow: 0.2rem 0.8rem 1.6rem var(--colorPrimary600); */
          
          &-heading {
            display: flex;
            flex-direction: row;
            justify-content: space-between;
            width: 100%;
            margin-bottom: 35px;
  
