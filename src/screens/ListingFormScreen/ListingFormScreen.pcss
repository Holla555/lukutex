  }
  .button-submit {
    display: flex;
    justify-content: center;
    margin: 40px 0;

    .btn-button {
      width: 250px;
      margin-right: 0;
      padding: 15px 0 40px;
      background-color: #3067f0;
      font-size: 20px;
    }
